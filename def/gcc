#!/cw
path /bin:/usr/bin:/sbin:/usr/sbin:<env>
nopipe
base grey+
digit white:default
match yellow:default :
match white:default .
match white:default `
match white:default '
match white:default "
match white:default /
match white:default -
match yellow:default <
match yellow:default >
match yellow:default [
match yellow:default ]
match green+:default warning:
match red+:default error:
